#이진탐색트리란?
//왼쪽자식노드는 부모노드보다 작고, 오른쪽자식노드는 부모노드보다 큰 구조를 가지고 있는 트리
//이진탐색트리에서 어떤 수를 탐색할 때 찾기 유용하며 대소를 나뉘며 탐색해야하는 노드를 계속 반으로 줄여주기때문에
//탐색 시 시간복잡도는 O(logN)이다.

----------------------------------------------------------------------------------------------------------------------------------------
#배운점
//이진탐색트리에 관한 원리와 구현에 대해배웠다.
//이진탐색트리에 필요한 함수는 insert,delete,find 이 3개가 필수요소이다.
//find함수가 제일 만들기 쉬웠고 insert도 그렇게 어렵지않았지만 delete같은경우는 경우의수가 좀 많아서 다 생각하는데 시간이 좀걸렸다.
//delete 함수 만든 거 아마 더 깔끔하게 중복많
----------------------------------------------------------------------------------------------------------------------------------------
#출력
<트리출력>
10
5 17 
1 7 14 21 
6 이진탐색트리에서 10 탐색
트리에서 '10' 탐색중...
1번째 탐색후 찾았음. 

<17 삭제>
<트리출력>
10
5 14 
1 7 21 6 

<14 삭제>
<트리출력>
10
5 21 
1 7 6 

<10 삭제>
<트리출력>
7
5 21 
1 6
----------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include <stdlib.h>

typedef struct Node{
    int data;
    struct Node* left;
    struct Node* right;
}Node;

typedef struct node{
  Node *link;
  struct node* next;
}node;

typedef struct BST{
    Node *h;
}BST;

typedef struct Que{
    node *head;
    node *front;
    node *rear;
}Que;

Que *Q=NULL;

Node* Create_Node(int data){ // 트리용
    Node *newNode=(Node*)malloc(sizeof(Node));
    newNode->data=data;
    newNode->left=NULL;
    newNode->right=NULL;
    return newNode;
}

node* create_node(Node* link){ //큐용
    node *newNode=(node*)malloc(sizeof(node));
    newNode->link=link;
    newNode->next=NULL;
    return newNode;
}

void Enqueue(Node *link){//젤 뒤 노드에 붙여준다.
    node *newNode=create_node(link);
   
    if(Q->front==NULL){
        Q->head=newNode;
        Q->front=newNode;
        Q->rear=newNode;
        return;
    }
    else{
        Q->rear->next=newNode;
        Q->rear=newNode;
        return;
    }
}


Node* Dequeue(){//젤앞노드것을 뺀다.
    if(Q->front==NULL){
        printf("아무것도 없습니다.\n");
        return NULL;
    }
    Node* value;
    node *old_front;
    old_front=Q->front;
    value=Q->front->link;
    Q->head=Q->head->next;
    Q->front=Q->front->next;
    free(old_front);
    if(Q->front==NULL)Q->rear=NULL;
    return value;
}

int peak(){//젤 앞의 값을 보여줌
    if(Q->front==NULL){
        printf("아무것도 없습니다.\n");
    }
    return Q->front->link->data;
}

void BST_Insert(BST *H, int num){
    Node *p;
    Node *q;
    Node *newNode=Create_Node(num);
    if(H->h==NULL){
        H->h=newNode;
        return ;
    }
    p=H->h;
    while(p){
        if(num==p->data)return;
        q=p;
        if(num < p->data){
            p=p->left;
        }
        else p=p->right;
    }
    if(num<q->data){
        q->left=newNode;
    }
    else q->right=newNode;
}


void inorder(Node* node){
    if(node){
        inorder(node->left);
        printf("%d -> ",node->data);
        inorder(node->right);
    }
}



void BFS_TREE(BST *H){//BFS
  printf("<트리출력>\n");
  int i=1,j=2;
  Node *w;
  Node *v;
  v=H->h;
  Enqueue(v);
  printf("%d\n",v->data);
  while(Q->front!=NULL){
    v=Dequeue();
    if(v->left!=NULL){
      Enqueue(v->left);
      printf("%d ",v->left->data);
      i++;
      }
    if(v->right!=NULL){
      Enqueue(v->right);
      printf("%d ",v->right->data);
      i++;
      }
    if(i+1==j*2){
      printf("\n");
      j=i+1;
    }
  }
}

Node* BST_Find(BST *H, int data){
  printf("트리에서 '%d' 탐색중...\n");
  Node *horse;
  horse=H->h;
  int i=0;
  while(horse!=NULL){
    i++;
    if(horse->data==data){
      printf("%d번째 탐색후 찾았음. \n",i);
      return horse;
    }
    if(data<horse->data)horse=horse->left;
    else horse=horse->right;
  }
  printf("없음\n");
  return NULL;
}

void BST_delete(BST *H, int data){
  Node *parent=H->h;
  Node *p=H->h;
  Node *horse=H->h;
  if(data==H->h->data){ // 삭제할 노드가 루트노드일경우
    parent=p;
    p=p->left;
    while(p->right!=NULL){
      parent=p;
      p=p->right;
    }
    Node *newNode=Create_Node(p->data);
    newNode->left=H->h->left;
    newNode->right=H->h->right;
    BST_delete(H,p->data);
    horse=H->h;
    H->h=newNode;
    free(horse);
    return;
  }
  while(p!=NULL){ //삭제할노드의 위치와 그의 부모노드의위치를 찾기.
    if(p->data==data){
      break;
    }
    if(data < p->data){
      parent=p;
      p=p->left;
      }
    else{
      parent=p;
      p=p->right;
    }
  }
  if(p->left!=NULL && p->right!=NULL) {// 삭제할 노드의 자식이 두개일때
    if(parent->left->data==p->data){//삭제할 노드가 부모의 왼쪽일경우
      p->left->right=p->right; // 삭제할 노드의 왼쪽자식이 부모가 되고 오른쪽자식이 그 자식의 오른쪽 자식이 될거기때문.
      parent->left=p->left;
      free(p);
    }
    else{ //삭제할 노드가 부모의 오른쪽일경우
      p->left->right=p->right;
      parent->right=p->left;
      free(p);
    }
  }
  else if(p->left!=NULL || p->right!=NULL){ //삭제할 노드가 자식노드가 하나일경우exclusive OR(이유는 위에서 AND를 이미했으므로)
    if(parent->left->data == p->data){ //삭제할 노드가 부모의 왼쪽일경우
      if(p->left!=NULL){ // 삭제할 노드의 왼쪽에 자식노드가있을경우
        parent->left=p->left;
        free(p);
      }
      else{ // 삭제할 노드의 오른쪽에 자식노드가 있을경우
        parent->left=p->right;
      }
    }
    else{ // 삭제할 노드가 부모의 오른쪽일경우
      if(p->left!=NULL){// 삭제할 노드의 왼쪽에 자식노드가 있을경우
        parent->right=p->left;
        free(p);
      }
      else{// 삭제할 노드의 오른쪽에 자식노드가 있을경우
        parent->right=p->right;
      }
    }
  }
  else{ // 삭제할 노드가 자식노드가 하나도없을 경우
    if(parent->left->data == data){
      parent->left=NULL;
    }
    else{
      parent->right=NULL;
    }
  }
  
  //printf("없음\n");
}

int main(){
    Q=(Que*)malloc(sizeof(Que));
    BST *bstree=(BST*)malloc(sizeof(BST));
    BST_Insert(bstree,10);
    BST_Insert(bstree,5);
    BST_Insert(bstree,17);
    BST_Insert(bstree,14);
    BST_Insert(bstree,7);
    BST_Insert(bstree,1);
    BST_Insert(bstree,21);
    BST_Insert(bstree,6);
    //inorder(bstree->h);
  
    BFS_TREE(bstree);
    printf("이진탐색트리에서 10 탐색\n");
    BST_Find(bstree,10);
    printf("\n<17 삭제>\n");
    BST_delete(bstree,17);//자식 두개인 노드 삭제
    BFS_TREE(bstree);
    printf("\n<14 삭제>\n");
    BST_delete(bstree,14);//자식 한개인 노드 삭제
    BFS_TREE(bstree);
    printf("\n\n<10 삭제>");
    BST_delete(bstree,10);//루트노드 삭제
    printf("\n");
    BFS_TREE(bstree);
----------------------------------------------------------------------------------------------------------------------------------------

